@using TripGuide.Models.Class
@model List<About>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_PageLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>About</h3>
            @foreach (var item in Model)
            {
                <img src="@item.PhotoUrl" style="width:800px; height:400px;" alt="" />

                <p>@item.Description</p>
            }

        </div>
    </div>
</div>

