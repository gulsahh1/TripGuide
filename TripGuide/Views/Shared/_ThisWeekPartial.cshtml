@using TripGuide.Models.Class
@model List<Blog>

<div class="padding">
    <h4>This Week's Top 10</h4>
    <ul class="list list-pad">
        @{int i = 0;}
        @foreach (var item in Model)
        {
            <li class="list-bg"><span>@{i++;} @i.</span><a href="Blog/BlogDetail/@item.Id">@item.Title</a></li>
        }
    </ul>
    <a href="/Blog/Index/" class="link1">See All Blogs</a>
</div>
